<?xml version="1.0"?>
<root>
<root>
    <item>
        <name>Change Option_R Key (Right Option)</name>
        <item>
            <name>Option_R to Right Arrow</name>
            <identifier>private.optionR2ra</identifier>
            <autogen>
                --KeyToKey--
                KeyCode::OPTION_R,
                KeyCode::CURSOR_RIGHT
            </autogen>
        </item>
        <item>
            <name>Right CMD to Left Arrow</name>
            <identifier>private.optionRCmd2la</identifier>
            <autogen>
                --KeyToKey--
                KeyCode::COMMAND_R,
                KeyCode::CURSOR_LEFT
            </autogen>
        </item>
    </item>
</root>
</root>
